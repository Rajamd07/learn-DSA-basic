class Hello
{
	public static void main(String[] args) {
	int[] num={44,34,64,1,-19,6,4,9};
	System.out.println(min(num));
	System.out.println(max(num));
	}

	// find min from array
	static int min(int[] arr)
	{
		int ans=arr[0];
		for(int i=0;i<arr.length;i++)
		{
			if(arr[i]<ans)
				ans=arr[i];
		}
		return ans;

	}

	// find max from array
	static int max(int[] arr)
	{
		int ans=arr[0];
		for(int i=0;i<arr.length;i++)
		{
			if(arr[i]>ans)
				ans=arr[i];
		}
		return ans;

	}
}
